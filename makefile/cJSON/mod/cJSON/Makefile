
#----------------------------------------
# 目标名称
#----------------------------------------
target := libcJSON.a

#----------------------------------------
# 生成的静态库文件路径（采用相对路径）
#----------------------------------------
outdir := lib

#----------------------------------------
# 源文件路径（采用相对路径）
#----------------------------------------
srcdir := src source 

#----------------------------------------
# 头文件路径
#----------------------------------------
INCLUDE := -I./include 
INCLUDE += $(addprefix -I./, $(srcdir))

#----------------------------------------
# 编译参数
#----------------------------------------
CFLAGS := $(CFLAGS_ENV)

#----------------------------------------
# 目标生成
#----------------------------------------
-include $(mk_dir)/static_library.mk
