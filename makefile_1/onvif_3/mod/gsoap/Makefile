
#----------------------------------------
# 目标名称
#----------------------------------------
target := libgsoap.a

#----------------------------------------
# 生成的静态库文件路径（采用相对路径）
#----------------------------------------
outdir := lib

#----------------------------------------
# 源文件路径（采用相对路径）
#----------------------------------------
srcdir := src

#----------------------------------------
# 头文件路径
#----------------------------------------
INCLUDE := -I./include 
INCLUDE += $(addprefix -I./, $(srcdir))
INCLUDE += -I../openssl/include

#----------------------------------------
# 编译参数
#----------------------------------------
CFLAGS := $(CFLAGS_ENV)
CFLAGS += -g 
CFLAGS += -fPIC 
CFLAGS += -DWITH_DOM 
CFLAGS += -DWITH_OPENSSL 
CFLAGS += -DWITH_NONAMESPACE 
CFLAGS += -DDBUG 
CFLAGS += -DWITH_NO_C_LOCALE

#----------------------------------------
# 目标生成
#----------------------------------------
-include $(mk_dir)/static_library.mk

